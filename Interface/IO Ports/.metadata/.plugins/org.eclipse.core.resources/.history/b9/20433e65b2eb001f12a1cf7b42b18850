#include<avr/io.h>
#include<util/delay.h>

int main (void)
{
	DDRD |= 0xFF;  //Configure All the Pins in PORTD as Output pins
	PORTD = 0xF8;   //Turn OFF all the leds at the beginning by clearing all bits in PORTD

	while(1)
	{

	}
}
